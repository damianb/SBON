  "dependencies": {
    "@types/node": "^8.0.45",
    "ByteAccordion": "github:damianb/ByteAccordion",
    "big-integer": "^1.6.25"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-standard": "^6.0.1",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3"
  }
