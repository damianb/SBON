{
  "name": "sbon",
  "version": "2.3.6",
  "description": "SBON parsing library",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "docs": "typedoc ./src/index.ts",
    "style": "eslint ./src/*.ts",
    "unit": "cross-env NODE_ENV=test nyc mocha --config test/.mocharc.json --reporter-options configFile=test/mocha.json ./test/*.spec.ts",
    "test": "yarn run style && yarn run unit",
    "build": "tsc -p ./tsconfig.json",
    "qc": "yarn run build && yarn run style && yarn run unit",
    "prepublishOnly": "yarn run docs && yarn run build && yarn run style && yarn run unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damianb/SBON.git"
  },
  "keywords": [
    "SBON",
    "starbound"
  ],
  "author": "Damian Bushong <katana@odios.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damianb/SBON/issues"
  },
  "homepage": "https://github.com/damianb/SBON#readme",
  "dependencies": {
    "@types/node": "^14.14.22",
    "big-integer": "^1.6.43",
    "byteaccordion": "^1.1.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  }
}
