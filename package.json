  },
  "homepage": "https://github.com/damianb/SBON#readme",
  "dependencies": {
    "@types/node": "^12.0.7",
    "big-integer": "^1.6.43",
    "byteaccordion": "^1.1.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.17.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  }
