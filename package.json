{
  "name": "sbon",
  "version": "2.3.4",
  "description": "SBON parsing library",
  "main": "build/SBON.js",
  "types": "build/SBON.d.ts",
  "scripts": {
    "docs": "typedoc ./src/",
    "style": "eslint ./src/*.ts",
    "unit": "cross-env NODE_ENV=test nyc mocha --config test/.mocharc.json --reporter-options configFile=test/mocha.json ./test/*.spec.ts",
    "test": "npm run style && npm run unit",
    "build": "tsc -p ./tsconfig.json",
    "qc": "npm run build && npm run style && npm run unit",
    "prepublishOnly": "npm run docs && npm run build && npm run style && npm run unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damianb/SBON.git"
  },
  "keywords": [
    "SBON",
    "starbound"
  ],
  "author": "Damian Bushong <katana@odios.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damianb/SBON/issues"
  },
  "homepage": "https://github.com/damianb/SBON#readme",
  "dependencies": {
    "@types/node": "^14.6.4",
    "big-integer": "^1.6.43",
    "byteaccordion": "^1.1.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  }
}
