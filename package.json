  "description": "SBON parsing library",
  "main": "build/SBON.js",
  "types": "build/SBON.d.ts",
  "scripts": {
    "docs": "typedoc ./src/",
    "style": "tslint ./src/*.ts -p ./tsconfig.json",
    "unit": "mocha --require ts-node/register ./test/*.spec.ts",
